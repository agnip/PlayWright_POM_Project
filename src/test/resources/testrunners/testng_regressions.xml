<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">


<suite name="Swag Lab Demo Playwright Project" thread-count="5"
	parallel="tests" verbose="4">

	<listeners>
		<listener
			class-name="com.qa.swag.listeners.ExtentReportListener" />

	</listeners>

	<test name="Open Cart Home Page Test_chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.qa.swag.tests.LoginPageTest" />
		</classes>
	</test>
</suite>